{
  "version": "1.1.0",
  "license": "MIT",
  "name": "aws-elasticsearch-model",
  "author": "Dmitriy Nevzorov",
  "repository": "https://github.com/jimmyn/aws-elasticsearch-model",
  "module": "dist/aws-elasticsearch-model.esm.js",
  "keywords": [
    "aws",
    "elasticsearch",
    "serverless",
    "aws-lambda",
    "aws-elasticsearch"
  ],
  "description": "Simplifies AWS ElasticSearch Service integration into serverless applications built with AWS Lambda",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "release": "release-it"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "bracketSpacing": false,
    "printWidth": 100
  },
  "release-it": {
    "git": {
      "tagName": "v${version}",
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "hooks": {
      "before:init": [
        "yarn lint",
        "CI=true yarn test"
      ]
    }
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.40",
    "@types/elasticsearch": "^5.0.36",
    "@types/faker": "^4.1.9",
    "@types/http-aws-es": "^6.0.0",
    "@types/jest": "^25.1.0",
    "aws-lambda": "1.0.5",
    "aws-sdk": "^2.610.0",
    "dotenv": "^8.2.0",
    "faker": "latest",
    "husky": "^4.2.1",
    "release-it": "^12.4.3",
    "tsdx": "^0.12.3",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "bodybuilder": "^2.2.20",
    "dynamodb-data-types": "^3.0.1",
    "elasticsearch": "16.6.0",
    "http-aws-es": "^6.0.0"
  }
}
